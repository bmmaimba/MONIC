{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.AspNetCore.SpaProxy": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "ConnectionStrings": {
    "DefaultConnection": "Data Source={SET_THIS};Initial Catalog={SET_THIS};Integrated Security=True;MultipleActiveResultSets=true;"
  },
  "Settings": {
    "RootUrl": "https://localhost:{SET_THIS}/",
    "RootPath": "<gets set at startup>",
    "SiteName": "{SET_THIS}",
    "IsDevelopment": true,
    "UseDevelopmentCertificate": false,
    "UseApplicationInsights": false,
    "UseSpaStaticFiles": true,
    "AccessTokenExpiryMinutes": 10,
    "RefreshTokenExpiryMinutes": 10880,
    "CertificatePassword": "{SET_THIS}",
    "ProxyToSpaDevelopmentServer": "",
    "EmailSettings": {
      "SenderName": "some name",
      "Sender": "some_email@some_server.com",
      "SendGridKey": "",
      "SubstitutionEmailAddress": "your@email.com",
      "EmailToErrors": "your@email.com",
      "SendEmails": false,
      "SendErrorEmails": false
    }
  }
}
