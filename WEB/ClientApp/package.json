{
    "name": "monic",
    "version": "0.0.0",
    "scripts": {
        "ng": "ng",
        "prestart": "node aspnetcore-https",
        "start": "run-script-os",
        "start:windows": "ng serve --port 44410 --live-reload=true --ssl --ssl-cert %APPDATA%\\ASP.NET\\https\\%npm_package_name%.pem --ssl-key %APPDATA%\\ASP.NET\\https\\%npm_package_name%.key",
        "start:default": "ng serve --port 44410 --live-reload=false --ssl --ssl-cert $HOME/.aspnet/https/${npm_package_name}.pem --ssl-key $HOME/.aspnet/https/${npm_package_name}.key",
        "build": "ng build",
        "build:ssr": "ng run Monic:server:dev",
        "watch": "ng build --watch --configuration development",
        "test": "ng test"
    },
    "private": true,
    "dependencies": {
        "@angular/animations": "15.2.9",
        "@angular/cdk": "15.2.9",
        "@angular/common": "15.2.9",
